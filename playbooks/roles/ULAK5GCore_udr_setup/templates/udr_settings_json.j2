{
    "Database": {
        "ConnectionPoolSize": 64,
        "ConnectionRetryPeriod": 120000,
        "ConnectionTimeout": 3,
        "DatabaseName": "{{ db_name }}",
        "DatabaseType": "POSTGRESQL",
        "Password": "{{ postgre_db_password }}",
        "Port": {{ postgre_db_port }},
        "Server": "{{ postgre_db_ip }}",
        "SubscriptionCheckPeriod": 60000,
        "UserName": "{{ postgre_db_username }}"
    },
    "General": {
        "ClientCount": 6,
        "ClientTLSMutual": false,
        "ClientTimeout": 3000,
        "DataRepositoryCustomIP": "{{ control_ip }}",
        "DataRepositoryCustomOAuth2Enabled": false,
        "DataRepositoryCustomPort": 5422,
        "DataRepositoryCustomTLSEnabled": false,
        "PrometheusIP": "0.0.0.0",
        "PrometheusPort": 5019,
        "PrometheusTLSEnabled": false,
        "PrometheusThreadCount": 1,
        "SettingsIP": "{{ control_ip }}",
        "SettingsOAuth2Enabled": false,
        "SettingsPort": 5421,
        "SettingsTLSEnabled": true,
        "SettingsTLSMutualAuth": false,
        "SettingsThreadCount": 1,
        "SubscriptionIP": "{{ control_ip }}",
        "SubscriptionPort": 5423,
        "SubscriptionTLSEnabled": false,
        "SubscriptionTLSMutualAuth": false,
        "SubscriptionThreadCount": 1,
        "homePlmnId": {
            "mcc": "{{ h_mcc | default("001") }}",
            "mnc": "{{ h_mnc | default("002") }}"
        }
    },
    "Logging": {
        "Directory": "/var/log/cinar/udr/",
        "DisplayLog": false,
        "FileName": "cnrudr",
        "LogLevel": "DEBUG"
    },
    "Nrf": {
        "ClientCount": 1,
        "ClientTimeout": 3000,
        "DiscServicePort": 8006,
        "FQDN": "",
        "IPAddress": "{{ nrf_ip }}",
        "NfmServicePort": 8001,
        "OAuth2ServicePort": 8007,
        "TAccessTokenPeriod": 1000,
        "TCheckPeriod": 60000,
        "TDiscoveryPeriod": 3000,
        "TLSMutualAuth": false,
        "TLSSecure": false,
        "TRetryPeriod": 3000,
        "TSubscriptionPeriod": 10000
    },
    "Security": {
        "OAuth2": {
            "PublicKeyFile": "/opt/cinar/certificate/jwt.pub"
        },
        "TLS": {
            "PrivateKeyFile": "/opt/cinar/certificate/localhost.key",
            "PublicKeyFile": "/opt/cinar/certificate/localhost.crt"
        }
    },
    "nudr-dr": {
        "MutualAuthenticate": false,
        "NumberOfServiceThreads": 12,
        "OAuth2Enabled": true,
        "VersionFull": "2.0.1.alpha-1",
        "VersionInUri": "v2"
    }
